\long\def\@@EMparbox<#1>[#2]#3{%
  \@ifundefined{varwidth}{\errmessage{EMparbox は varwidth.sty を必要とします。}}{}%
  \define@key{emath}{width}{%
    \headchar{##1}\tmp@h\tmp@r
    \if\tmp@h -
      \setlength{\@tempdima}{\linewidth-\tmp@r}%
      \edef\EMpb@w{\the\@tempdima}%
    \else
      \edef\EMpb@w{##1}%
    \fi
  }%
  \leavevmode%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
  \ifthenelse{\equal{#1}\empty}{%
    \begin{varwidth}[#2]{\linewidth}%
      \leavevmode #3\relax
    \end{varwidth}%
  }{%
    \ifthenelse{\equal{#1}{t}}{%
      \@@EMparbox@<#1>[#2]{#3}%
    }{%
      \setkeys{emath}{#1}%
      \begin{varwidth}[#2]{\EMpb@w}%
        \leavevmode #3\relax
      \end{varwidth}%
    }%
  }%
  \ignorespaces
}%
